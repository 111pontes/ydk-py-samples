<ipv4-acl-and-prefix-list xmlns="http://cisco.com/ns/yang/Cisco-IOS-XR-ipv4-acl-cfg">
  <prefixes>
    <prefix>
      <prefix-list-name>PREFIX-LIST3</prefix-list-name>
      <prefix-list-entries>
        <prefix-list-entry>
          <sequence-number>10</sequence-number>
          <remark>block a prefix</remark>
        </prefix-list-entry>
        <prefix-list-entry>
          <sequence-number>20</sequence-number>
          <grant>deny</grant>
          <netmask>255.255.255.0</netmask>
          <prefix>172.31.0.0</prefix>
        </prefix-list-entry>
        <prefix-list-entry>
          <sequence-number>30</sequence-number>
          <grant>permit</grant>
          <match-max-length></match-max-length>
          <max-prefix-length>32</max-prefix-length>
          <netmask>0.0.0.0</netmask>
          <prefix>0.0.0.0</prefix>
        </prefix-list-entry>
      </prefix-list-entries>
    </prefix>
  </prefixes>
</ipv4-acl-and-prefix-list>

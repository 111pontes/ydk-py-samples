<network-instances xmlns="http://openconfig.net/yang/network-instance">
  <network-instance>
    <name>default</name>
    <config>
      <name>default</name>
    </config>
    <mpls>
      <lsps>
        <constrained-path>
          <tunnels>
            <tunnel>
              <name>LER1-LER2-t52</name>
              <config>
                <name>LER1-LER2-t52</name>
                <type xmlns:oc-mpls-types="http://openconfig.net/yang/mpls-types">oc-mpls-types:P2P</type>
              </config>
              <bandwidth>
                <auto-bandwidth>
                  <config>
                    <enabled>true</enabled>
                    <min-bw>10000</min-bw>
                    <max-bw>500000</max-bw>
                  </config>
                  <overflow>
                    <config>
                      <overflow-threshold>15</overflow-threshold>
                      <trigger-event-count>3</trigger-event-count>
                    </config>
                  </overflow>
                  <underflow>
                    <config>
                      <underflow-threshold>15</underflow-threshold>
                      <trigger-event-count>3</trigger-event-count>
                    </config>
                  </underflow>
                </auto-bandwidth>
              </bandwidth>
              <p2p-tunnel-attributes>
                <config>
                  <destination>172.16.255.2</destination>
                </config>
                <p2p-primary-path>
                  <p2p-primary-path>
                    <name>DYNAMIC</name>
                    <config>
                      <name>DYNAMIC</name>
                      <path-computation-method xmlns:oc-mpls-types="http://openconfig.net/yang/mpls-types">oc-mpls-types:LOCALLY_COMPUTED</path-computation-method>
                      <preference>10</preference>
                    </config>
                  </p2p-primary-path>
                </p2p-primary-path>
              </p2p-tunnel-attributes>
            </tunnel>
          </tunnels>
        </constrained-path>
      </lsps>
    </mpls>
  </network-instance>
</network-instances>

<router-static xmlns="http://cisco.com/ns/yang/Cisco-IOS-XR-ip-static-cfg">
  <default-vrf>
    <address-family>
      <vrfipv6>
        <vrf-unicast>
          <vrf-prefixes>
            <vrf-prefix>
              <prefix>2001:db8:a::</prefix>
              <prefix-length>64</prefix-length>
              <vrf-route>
                <vrf-next-hop-table>
                  <vrf-next-hop-interface-name>
                    <interface-name>Null0</interface-name>
                  </vrf-next-hop-interface-name>
                </vrf-next-hop-table>
              </vrf-route>
            </vrf-prefix>
          </vrf-prefixes>
        </vrf-unicast>
      </vrfipv6>
    </address-family>
  </default-vrf>
</router-static>


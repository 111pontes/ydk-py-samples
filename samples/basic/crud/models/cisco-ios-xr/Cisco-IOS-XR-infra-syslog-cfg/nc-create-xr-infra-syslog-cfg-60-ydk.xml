<syslog xmlns="http://cisco.com/ns/yang/Cisco-IOS-XR-infra-syslog-cfg">
  <suppression xmlns="http://cisco.com/ns/yang/Cisco-IOS-XR-infra-correlator-cfg">
    <rules>
      <rule>
        <name>RULE1</name>
        <applied-to>
          <all/>
        </applied-to>
        <alarm-causes>
          <alarm-cause>
            <category>MGBL</category>
            <group>CONFIG</group>
            <code>DB_COMMIT</code>
          </alarm-cause>
          <alarm-cause>
            <category>SECURITY</category>
            <group>LOGIN</group>
            <code>AUTHEN_SUCCESS</code>
          </alarm-cause>
        </alarm-causes>
      </rule>
    </rules>
  </suppression>
</syslog>


<ipv4-acl-and-prefix-list xmlns="http://cisco.com/ns/yang/Cisco-IOS-XR-ipv4-acl-cfg">
  <accesses>
    <access>
      <access-list-name>ACL3</access-list-name>
      <access-list-entries>
        <access-list-entry>
          <sequence-number>10</sequence-number>
          <remark>log access attempts</remark>
        </access-list-entry>
        <access-list-entry>
          <sequence-number>20</sequence-number>
          <destination-network>
            <destination-address>172.16.0.0</destination-address>
            <destination-prefix-length>16</destination-prefix-length>
          </destination-network>
          <grant>deny</grant>
          <log-option>log-input</log-option>
          <source-network>
            <source-address>173.31.1.0</source-address>
            <source-prefix-length>24</source-prefix-length>
          </source-network>
        </access-list-entry>
        <access-list-entry>
          <sequence-number>30</sequence-number>
          <destination-network>
            <destination-address>172.16.0.0</destination-address>
            <destination-prefix-length>16</destination-prefix-length>
          </destination-network>
          <dscp>
            <dscp-min>ef</dscp-min>
          </dscp>
          <grant>deny</grant>
          <log-option>log-input</log-option>
          <source-network>
            <source-address>172.31.2.0</source-address>
            <source-prefix-length>24</source-prefix-length>
          </source-network>
        </access-list-entry>
        <access-list-entry>
          <sequence-number>40</sequence-number>
          <counter-name>ACL3-COUNTER</counter-name>
          <destination-network>
            <destination-address>172.16.0.0</destination-address>
            <destination-prefix-length>16</destination-prefix-length>
          </destination-network>
          <grant>deny</grant>
          <log-option>log-input</log-option>
          <source-network>
            <source-address>172.31.3.0</source-address>
            <source-prefix-length>24</source-prefix-length>
          </source-network>
        </access-list-entry>
        <access-list-entry>
          <sequence-number>50</sequence-number>
          <grant>permit</grant>
        </access-list-entry>
      </access-list-entries>
    </access>
  </accesses>
</ipv4-acl-and-prefix-list>

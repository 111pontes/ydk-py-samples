<ipv4-acl-and-prefix-list xmlns="http://cisco.com/ns/yang/Cisco-IOS-XR-ipv4-acl-cfg">
  <prefixes>
    <prefix>
      <prefix-list-name>PREFIX-LIST4</prefix-list-name>
      <prefix-list-entries>
        <prefix-list-entry>
          <sequence-number>10</sequence-number>
          <remark>allow prefix ranges</remark>
        </prefix-list-entry>
        <prefix-list-entry>
          <sequence-number>20</sequence-number>
          <grant>permit</grant>
          <match-min-length></match-min-length>
          <min-prefix-length>24</min-prefix-length>
          <netmask>255.255.0.0</netmask>
          <prefix>172.17.0.0</prefix>
        </prefix-list-entry>
        <prefix-list-entry>
          <sequence-number>30</sequence-number>
          <grant>permit</grant>
          <match-max-length></match-max-length>
          <max-prefix-length>24</max-prefix-length>
          <netmask>255.255.0.0</netmask>
          <prefix>172.18.0.0</prefix>
        </prefix-list-entry>
        <prefix-list-entry>
          <sequence-number>40</sequence-number>
          <grant>permit</grant>
          <match-max-length></match-max-length>
          <match-min-length></match-min-length>
          <max-prefix-length>20</max-prefix-length>
          <min-prefix-length>28</min-prefix-length>
          <netmask>255.255.0.0</netmask>
          <prefix>172.19.0.0</prefix>
        </prefix-list-entry>
      </prefix-list-entries>
    </prefix>
  </prefixes>
</ipv4-acl-and-prefix-list>
